n = int(input("Enter number of elements: "))
arr = []

for i in range(n):
    num = int(input("Enter element: "))
    arr.append(num)

print("Sum =", sum(arr))

#input
#Enter number of elements: 5    
#Enter element: 10
#Enter element: 20
#Enter element: 30
#Enter element: 40
#Enter element: 50
#output
#Sum = 150  
n = int(input("Enter number of elements: "))
arr = []

for i in range(n):
    num = int(input("Enter element: "))
    arr.append(num)

arr = list(set(arr))
arr.sort()

print("Second largest element =", arr[-2])

#input
#Enter number of elements: 5
#Enter element: 10
#Enter element: 20
#Enter element: 30
#Enter element: 40
#Enter element: 50

#output
#Second largest element = 40

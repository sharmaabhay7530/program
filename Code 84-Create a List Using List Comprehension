n = int(input("Enter number of elements: "))

numbers = [int(input("Enter element: ")) for _ in range(n)]

print("List =", numbers)

#input
#Enter number of elements: 5
#Enter element: 10
#Enter element: 20
#Enter element: 30
#Enter element: 40
#Enter element: 50

#output
#List = [10, 20, 30, 40, 50]

def maximum(a, b, c):
    return max(a, b, c)

x = int(input("Enter first number: "))
y = int(input("Enter second number: "))
z = int(input("Enter third number: "))

print("Maximum =", maximum(x, y, z))

#input
#Enter first number: 10
#Enter second number: 20
#Enter third number: 30

#output
#Maximum = 30
